                <template v-if="!name">
                    {{ hash }}
                </template>
                <template v-else>
                    <span class="mr-5 as-center c-txt-main"
                          :class="{ icon: icon === 'icon-exchange-uniswap' }">
                        <template v-if="icon">
                            <img :src="`${CDN}/assets/svg/${icon}.svg`" width="auto" height="auto" />
                        </template>
                    </span>
                    <span class="font-p fs-14 lh-100 medium c-txt-main d-iblock va-mid">{{ name }}</span>
                </template>
            </a>
            <template v-if="!nocopy">
                <span class="d-iblock va-mid hash__copy hover-pointer"
                      v-copy="`${hash}`"
                      v-tooltip="'Click para copiar'"
                      :data-v-tooltip-clicked="'Copiado'">
                    <svg width="18" height="18" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <g opacity="0.25">
                            <rect x="5" y="1" width="14" height="14" rx="3" stroke="currentColor" stroke-width="2"/>
                            <path fill-rule="evenodd" clip-rule="evenodd" d="M4 4C1.79086 4 0 5.79086 0 8V16C0 18.2091 1.79086 20 4 20H12C14.2091 20 16 18.2091 16 16H14C14 17.1046 13.1046 18 12 18H4C2.89543 18 2 17.1046 2 16V8C2 6.89543 2.89543 6 4 6V4Z" fill="currentColor"/>
                        </g>
                    </svg>
                </span>
            </template>
            <template v-else>
            </template>
            <template v-if="isChange">
                <span class="hash__badge--change d-inline va-mid font-h medium ml-5 br-15 c-txt-main"
                      v-tooltip="``">Cambio</span>
            </template>
                    </div>
    </template>
</hash-expanded>
                                                </template>
                                                <template v-else-if="column && column.hasOwnProperty('default')">{{ column.default }}</template>
                                                <template v-else>–</template>
                                            </template>

                                            <template v-else-if="column.format === 'image'">
                                                image
                                            </template>

                                            <template v-else>
                                                <value
    class="" :value="row && row[field]" :format="column.format" :link="compileUrl(column.url, Object.assign({}, row, {value: (row && row[field])}))" :class="column.style && column.style === 'gray' ? 'c-txt-quiet-gray' : 'null'" :tooltip-placement="(column && column.hasOwnProperty('tooltip_placement')) ?
                                                        column['tooltip_placement'] :
                                                        (
                                                            (index === 0 || index === 1)
                                                                ? (
                                                                    (idx === 0)
                                                                        ? 'bottom-left'
                                                                        : 'top-left'
                                                                )
                                                                : (
                                                                    (index === columns.length - 1)
                                                                        ? (
                                                                            (idx === 0)
                                                                                ? 'bottom-right'
                                                                                : 'top-right'
                                                                        )
                                                                        : (
                                                                            (idx === 0)
                                                                                ? 'bottom'
                                                                                : 'top'
                                                                        )
                                                                )
                                                        )" :verified="column.verified ? compileString(column.verified, row) : null"
    v-slot="{ value, change, change24h, defaultValue }"
    field=""
    :format='null'
>
    </value>
                                            </template>

                                        </template>

                                    </td>

                                </template>

                            </tr>
                            <tr v-if="items && items.length < 1">
                                <td colspan="8" class="font-p regular fs-13 lh-auto ls-2 ta-center">
