        <script type="text/javascript" nonce="E90ei0QGNbtBNFHCwBlIBJvZ7eJjYzAl">

    window.APP_MOUNTED = false

    window.redirectNojs = function() {

        var xhr = new XMLHttpRequest();
        xhr.open("POST", '/monkey/no-js/1', true);
        xhr.setRequestHeader('X-CSRF-TOKEN', 'FNo6e6bGUX36HaFEPyIT0RTR4KprJT5LWhpSERZc');

        xhr.onreadystatechange = function() {
            if (xhr.readyState === 4) {
                location.reload()
            }
        }

        xhr.send();
    }

    var alreadyChanged = [];

    window._err = [];

    window.onError = function(e) {

        window._err.push(e);

        console.warn('error', e);

        if(e.target && e.target.nodeName) {

            if(e.target.getAttribute('data-fallback') !== "/" && window.Blockchair.CDN && window.Blockchair.CDN !== "" && window.Blockchair.CDN !== "/") {

                switch(e.target.nodeName) {
                    case 'IMG':

                        if(e.target.src.indexOf(window.Blockchair.CDN) !== -1) {
                            e.target.setAttribute("data-fallback", "/")
                            e.target.src = e.target.src.replace(/^.*\/\/[^\/]+/, '');
                        }

                        break;
                    case 'SCRIPT':

                        if(e.target.src.indexOf(window.Blockchair.CDN) !== -1) {
                            e.target.setAttribute("data-fallback", "/")
                            e.target.src = e.target.src.replace(/^.*\/\/[^\/]+/, '');

                            var parentEl = e.target.parentElement
                            parentEl.removeChild(e.target)

                            var newEl = document.createElement('script');
                            newEl.type='text/javascript';
                            newEl.async = true;
                            newEl.defer = true;
                            newEl.src = e.target.src;
                            newEl.nonce = 'E90ei0QGNbtBNFHCwBlIBJvZ7eJjYzAl';
                            setTimeout(function(){
                                document.body.appendChild(newEl);
                            }, 100);
                        }

                        break;
                    case 'LINK':

                        if(e.target.getAttribute('rel') !== 'preload' && e.target.href.indexOf(window.Blockchair.CDN) !== -1) {

                            e.target.href = e.target.href.replace(/^.*\/\/[^\/]+/, '');

                            // replace background images (through dirty check if we deal with main app.css file)
                            if(e.target.href.indexOf('.css') !== -1 && e.target.href.indexOf('app.') !== -1) {
                                setTimeout(function () {
                                    if(document.querySelectorAll("*")) {
                                        document.querySelectorAll("*").forEach(function (el) {
                                            var bg = el.style.backgroundImage || el.style.background || null;
                                            if (bg && bg.indexOf(window.Blockchair.CDN) !== -1) {
                                                el.style.backgroundImage = bg.replace(window.Blockchair.CDN, "");
                                            }
                                        });
                                    }
                                }, 1024)
                            }
                        }

                        break;
                }

            }
        }
        else {

            var errorString = e.error ? e.error.toString() : "unknown error";
            var errorStack = (e.error && e.error.stack) ? e.error.stack.toString() : "unknown stack";

            if(!window.APP_MOUNTED) {


                var errorTypesForReload = ['referenceerror', 'syntaxerror', 'typeerror', 'evalerror', "unknown", "unexpected", "syntaxerror:", "invalid character"];

                for (var i = 0; i < errorTypesForReload.length; i++) {
                    if (errorString.toLowerCase().indexOf(errorTypesForReload[i]) !== -1) {

                        window.redirectNojs();
                        break;
                    }
                }

                            }

            console.warn('error no target', errorString)


            // track error
            var skip = false;

            var skippedStrings = ['_avast_submit', 'getReadModeConfig', 'getReadModeExtract', 'MetaMask', 'solana', 'chrome-extension://', '//a[@href]]', 'LumiProvider', 'articleReadFont is not defined', 'startLoad', 'zaloJSV2', 'googletag', '$ is not defined', 'webkitExitFullScreen'];

            var errorStringStack = errorString + errorStack

            for (var i = 0; i < skippedStrings.length; i++) {
                if(errorStringStack.indexOf(skippedStrings[i]) !== -1) {
                    skip = true;
                    break;
                }
            }

            if(!skip) {

                var xhre = new XMLHttpRequest();
                xhre.open("POST", '/monkey/error', true);
                xhre.setRequestHeader('X-CSRF-TOKEN', 'FNo6e6bGUX36HaFEPyIT0RTR4KprJT5LWhpSERZc');

                var data = new FormData();
                data.append('message', e.error.toString());
                data.append('context', JSON.stringify({
                    location: document.location.href,
                    browser: 'chrome ',
                    platform: 'Windows',
                    device: 'WebKit',
                    userAgent: navigator.userAgent,
                    line: e.error.line,
                    stack: e.error.stack
                }));

                xhre.send(data);
            }

                    }
    }

    window.addEventListener('error', window.onError, true);
</script>


